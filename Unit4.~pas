unit Unit4;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, ExtDlgs, DB, ADODB, StdCtrls, ComCtrls, jpeg, ExtCtrls;

type
  TForm4 = class(TForm)
    Image1: TImage;
    Button1: TButton;
    Button2: TButton;
    Button3: TButton;
    Button5: TButton;
    Button4: TButton;
    PageControl1: TPageControl;
    TabSheet4: TTabSheet;
    GroupBox6: TGroupBox;
    Image6: TImage;
    Label30: TLabel;
    Label31: TLabel;
    Label24: TLabel;
    Label28: TLabel;
    GroupBox7: TGroupBox;
    Image7: TImage;
    Label29: TLabel;
    Label32: TLabel;
    Label33: TLabel;
    Label34: TLabel;
    procedure Button1Click(Sender: TObject);
    procedure Button2Click(Sender: TObject);
    procedure Button3Click(Sender: TObject);
    procedure Button5Click(Sender: TObject);
    procedure Button4Click(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form4: TForm4;

implementation

uses Unit1, Unit2, Unit3, Unit5;

{$R *.dfm}

procedure TForm4.Button1Click(Sender: TObject);
begin
form1.Show;
form2.Hide;
form3.Hide;
form4.Hide;
end;

procedure TForm4.Button2Click(Sender: TObject);
begin
form1.Hide;
form2.Show;
form3.Hide;
form4.Hide;
end;

procedure TForm4.Button3Click(Sender: TObject);
begin
form1.Hide;
form2.Hide;
form3.Show;
form4.Hide;
end;

procedure TForm4.Button5Click(Sender: TObject);
begin
form1.Hide;
form2.Hide;
form3.Hide;
form4.Show;
end;

procedure TForm4.Button4Click(Sender: TObject);
begin
form1.Hide;
form2.Hide;
form3.Hide;
form4.Hide;
form5.Show;
end;

end.
